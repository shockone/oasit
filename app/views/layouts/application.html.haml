!!!
%html
  %head
    %title Oasit
    = stylesheet_link_tag    'application', :media => "all"
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body
    .container
      = render partial: 'layouts/navbar'
      - if notice
        %p.alert.alert-success= notice
      - if alert
        %p.alert.alert-danger= alert if alert

      = yield
